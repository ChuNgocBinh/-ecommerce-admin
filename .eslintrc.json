{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-filename-extension": [
            1,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "semi": 1,
        "quotes": [
            2,
            "single"
        ],
        "no-tabs": 0,
        "no-unused-vars": 0,
        "max-len": [
            0,
            {
                "code": 120,
                "tabWidth": 4
            }
        ],
        "import/no-unresolved": 0,
        "linebreak-style": 0,
        "no-console": 0,
        "implicit-arrow-linebreak": 0,
        "import/prefer-default-export": 0,
        "no-param-reassign": 0,
        "react/prop-types": 0,
        "react/no-array-index-key": 0,
        "camelcase": 0
    }
}